library(tidyverse)
library(polspline)
library(coloredICA)
library(fastICA)
library(Matrix)
u <- 2
t <- 200
# Set mean spiking interval for each component
mean_spike_interval <- c(30, 10)
spike_times <- list()
for(i in 1:u){
# Simulate spike times using poisson
set.seed(12)
spike_intervals <- rpois(t, mean_spike_interval[i])
spike_intervals
spike_times[[i]] <- cumsum(spike_intervals)
}
S <- do.call("rbind", spike_times)
#A <- diag(rep(5, u))
A <- rbind(c(1, 0.5), c(0.5, 1))
X <- A%*%S
# Whiten data
# Get matrix from cICA
cica_fit <- cICA(X)
X <- t(cica_fit$W)%*%S
